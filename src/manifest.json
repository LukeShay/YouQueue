{
    "manifest_version": 2,
    "name": "YouQueue",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["/styles/extensionUI.css"],
            "js": ["/scripts/content.js"]
        }
    ],
    "background": {
        "scripts": ["/scripts/background.js"]
    },
    "permissions": [
        "activeTab",
        "tabs"
      ],
    "browser_action": {
        "default_icon": "/resources/icons8-musical-notes-64.png"
    }
}